<%= form_for @video do |f| %>
	<p>
		<%= f.label :titulo %>
		<%= f.text_field :title %>
	</p>
	<p>
		<%= f.label :duracion %>
		<%= f.text_field :length %>
	</p>
	<p>
		<%= f.label :calorias %>
		<%= f.text_field :calories %>
	</p>
	<p>
		<%= f.label :url %>
		<%= f.text_field :url %>
	</p>
	<%= f.submit %>
<% end %>
